; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dinolike"
run/main_scene="res://game/game.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")

[autoload]

SpawnManager="*res://core/spawn_manager.gd"
Events="*res://core/events.gd"
Globals="*res://core/globals.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=320
window/size/window_width_override=900
window/size/window_height_override=750
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"

[layer_names]

2d_physics/layer_1="enviroment"
2d_physics/layer_2="dino"
2d_physics/layer_3="meteor"

[rendering]

textures/canvas_textures/default_texture_filter=3
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
